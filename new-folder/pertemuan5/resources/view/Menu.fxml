<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="com.gudang.controller.MenuController"
    prefWidth="200" prefHeight="300" alignment="TOP_CENTER" spacing="10">
    <children>
        <Label text="Main Menu" />
        <Button text="Product Management" onAction="#openProduct" />
        <Button text="Customer Management" onAction="#openCustomer" />
    </children>
    <padding>
        <Insets top="20" right="20" bottom="20" left="20" />
    </padding>
</VBox>